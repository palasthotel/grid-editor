{
  "name": "grid-editor",
  "version": "1.0.0",
  "description": "This is the editor JavaScript library.",
  "main": "src/index.js",
  "scripts": {
    "serve": "./node_modules/.bin/parcel serve demo/index.html -d dist/demo",
    "watch-script": "./node_modules/.bin/parcel watch src/index.js -d dist",
    "watch-style": "./node_modules/.bin/parcel watch src/style/*.scss -d dist",
    "build-scripts": "./node_modules/.bin/parcel build src/*.js -d dist",
    "build-styles": "./node_modules/.bin/parcel build src/style/*.scss -d dist",
    "build-demo": "./node_modules/.bin/parcel build demo/index.html -d dist/demo",
    "build": "npm run build-scripts && npm run build-styles && npm run build-demo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/palasthotel/grid-editor.git"
  },
  "author": "Edward Bock",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/palasthotel/grid-editor/issues"
  },
  "homepage": "https://github.com/palasthotel/grid-editor#readme",
  "parcelCleanPaths": [
    "dist/demo/*.js",
    "dist/demo/*.js.map",
    "dist/demo/*.css",
    "dist/demo/*.css.map"
  ],
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-clean-easy": "^1.0.2",
    "sass": "^1.25.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.8.2",
    "@types/react": "^16.9.18",
    "axios": "^0.19.1",
    "react": "^16.12.0",
    "react-beautiful-dnd": "^12.2.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  }
}
